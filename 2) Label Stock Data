ues\n","mask_d = df_plot['Label'] == -1\n","# hold\n","mask_h = df_plot['Label'] == 0\n","\n","# select all rows except the ones that are labeled as hold and out of range\n","masked_df = df_plot[~mask_d]\n","masked_df = df_plot[~mask_h]\n","\n","palette = {\n","  1: 'tab:green',\n","  0: 'tab:gray',\n","  -1: 'tab:blue',\n","  2: 'tab:red',\n","}\n","\n","'''\n","p = sns.lineplot(x=\"Date\", y=\"Close\", data=df_plot)\n","p = sns.scatterplot(x=\"Date\", y=\"Close\", data=df_plot, hue = 'Label', palette=palette)\n","p.set_title((f'Buy and Sell Points for {stock} for {days} Days'), size = 14, fontweight='bold')\n","p.set_xlabel(\"Date\", size = 14)\n","p.set_ylabel(\"Close Price\", size = 14)\n","p.set_xticks(p.get_xticks()[::x_ticks])\n","\n","# df_plot.head()\n","'''\n","\n","p = sns.lineplot(x=\"Date\", y=\"Close\", data=df_plot)\n","p = sns.scatterplot(x=\"Date\", y=\"Close\", data=masked_df, hue = 'Label', palette=palette)\n","p.set_title((f'{days} Days of {stock} Buy and Sell Points'), size = 14, fontweight='bold')\n","p.set_xlabel(\"Date\", size = 14)\n","p.set_ylabel(\"Close Price\", size = 14)\n","p.set_xticks(p.get_xticks()[::x_ticks])\n","#sns.scatterplot(x=\"Date\", y=\"Close\", data=df_plot, hue = 'Label', palette=palette)\n","\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":567},"id":"T7a1n7sxj4kT","executionInfo":{"status":"ok","timestamp":1713248955460,"user_tz":240,"elapsed":977,"user":{"displayName":"Emmy Veselinov","userId":"15991982869453245141"}},"outputId":"2f69d3f3-231c-4a7b-b421-85ab696cd958"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["[<matplotlib.axis.XTick at 0x7c6eb75f34c0>,\n"," <matplotlib.axis.XTick at 0x7c6eb75f3580>,\n"," <matplotlib.axis.XTick at 0x7c6eb7a97df0>,\n"," <matplotlib.axis.XTick at 0x7c6eb7a5aa10>,\n"," <matplotlib.axis.XTick at 0x7c6eb718f130>]"]},"metadata":{},"execution_count":41},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["# Label Time Series (Sell/Buy/Hold) Method 2\n","\n","Labeling algorithm with an 11-day sliding window that uses Pandas windowing operations."],"metadata":{"id":"t_Y90Ol2O8ll"}},{"cell_type":"code","source":["w_size = 11\n","total_days = len(df2.index)\n","\n","df2['Label'] = -1\n","\n","for w in df.Close.rolling(window=w_size):\n","  if (total_days > w_size and len(w) == 11):\n","    # find the min and max and their corresponding indices\n","    max_val = w.max()\n","    min_val = w.min()\n","    max_index = w.idxmax();\n","    min_index = w.idxmin();\n","\n","    # calculate the median info\n","    i = w_size//2\n","    mid_index = w.index[i]\n","    mid_value = df2.iloc[mid_index]\n","\n","    # label each day\n","    if (max_index == mid_index):\n","      # sell is labeled 2\n","      df2.at[mid_index, 'Label'] = 2\n","\n","    elif (min_index == mid_index):\n","      # buy is labeled 1\n","      df2.at[mid_index, 'Label'] = 1\n","\n","    else:\n","      # hold is labeled 0\n","      df2.at[mid_index, 'Label'] = 0\n","\n","\n","\n","df2.head(50)\n"],"metadata":{"id":"BERhGmxRA4ao","colab":{"base_uri":"https://localhost:8080/","height":1000},"executionInfo":{"status":"ok","timestamp":1713248960104,"user_tz":240,"elapsed":2685,"user":{"displayName":"Emmy Veselinov","userId":"15991982869453245141"}},"outputId":"9b5a6138-3873-4345-fe2e-726fdc616789"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["          Date    Open    High    Low   Close  Adj Close    Volume  \\\n","0   2004-04-12  159.71  161.18  89.29  160.12      89.62   3255780   \n","1   2004-04-13  160.20  160.61  88.43  158.91      88.95   5244226   \n","2   2004-04-14  157.31  160.31  88.05  160.04      89.58   5560850   \n","3   2004-04-15  160.19  160.69  88.97  159.60      89.34   7159033   \n","4   2004-04-16  157.65  157.73  87.04  157.61      88.22  11629428   \n","