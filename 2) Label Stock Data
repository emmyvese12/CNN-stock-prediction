hile days_in_file > window_size:\n","  row_counter+=1;\n","  # check if a window can be made\n","  if ((days_in_file + 1) == row_counter):\n","    break\n","  elif (row_counter >= window_size):\n","    min = math.inf\n","    max = -1\n","    min_i = 0\n","    max_i = 0\n","\n","    begin_i = row_counter - window_size # 0\n","    end_i = begin_i + (window_size - 1) # 10\n","    middle_i = (begin_i + end_i)/2 # 5\n","    middle_i = int(middle_i)\n","\n","    # find the min and max prices of the window\n","    for i in range(begin_i, end_i + 1): # 0 -> 11\n","      #stock_price = df.iloc[i, 'Close']\n","      stock_price = df.iat[i, 4]\n","      # print(stock_price)\n","      # access values by integer position\n","      if stock_price < min:\n","        min = stock_price\n","        min_i = i\n","      if stock_price > max:\n","        max = stock_price\n","        max_i = i\n","\n","    # set labels\n","    # sell is labeled as 2\n","    if (max_i == middle_i):\n","      df.at[middle_i, 'Label'] = 2\n","      #df.loc[middle_i, 'Label'] = 'SELL'\n","\n","    # buy is labeled as 1\n","    elif (min_i == middle_i):\n","      df.at[middle_i, 'Label'] = 1\n","      #df.loc[middle_i, 'Label'] = 'BUY'\n","\n","    # hold is labeled as 0\n","    else:\n","      df.at[middle_i, 'Label'] = 0\n","      #df.loc[middle_i, 'Label'] = 'HOLD'\n","\n","\n","sell_count = df['Label'].value_counts()[2]\n","buy_count = df['Label'].value_counts()[1]\n","hold_count = df['Label'].value_counts()[0]\n","outside_range = days_in_file - (sell_count + buy_count + hold_count)\n","print(f'Days: {days_in_file}')\n","print(f'Sell Count: {sell_count} Buy Count: {buy_count} Hold Count: {hold_count}')\n","print(f'Outside the Range: {outside_range}\\n')\n","\n","df.to_csv(f'data/labeled/YahooFinance/{stock}.csv')\n","\n","df.head(50)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"FS-wOVy0LvLv","executionInfo":{"status":"ok","timestamp":1713248950033,"user_tz":240,"elapse